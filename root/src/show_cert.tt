[% INCLUDE form_header.tt %]
<form action="" method="post">
    <center>
        <fieldset>  
            <legend>Valid Certificate Found.</legend>  
            <table>
                <tr>
                    <td align='left'> 
                        <nobr>You seem to already have a certificate.</nobr><br>
                        <nobr>You may revoke it if you would like.</nobr><br>
                        <nobr>You should <b>*always*</b> revoke it if you suspect its private key has been compromised.</nobr><br>
                        <nobr>You will then be given the opportunity to regenerate a new certificate.</nobr><br>
                    </td>
                </tr>
                <tr>
                    <td align='right'><input type='submit' id='revoke' name='revoke' value='revoke'"></td>
                </tr>
                <tr>
                    <td align='center'> 
                        <a href="/?logout=1">Log out.</a>
                    </td>
                </tr>
            </table>
        </fieldset>
    </center>
</form>
